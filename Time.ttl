        shs:valueHum          "{nowHum: 75%, avgHum: 42%, maxHum: 75%, minHum: 40%}" ;
        shs:valueTep          "{nowTem: 24.6℃ avgTem: 24.5℃ maxTem: 25.0℃ minTem: 20.1℃}" ;
