@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix prov:  <http://www.w3.org/ns/prov#> .
@prefix foaf:  <http://xmlns.com/foaf/spec/20100809.html#> .
@prefix shs:   <http://example.org/#> .

shs:configData2 a prov:Entity ;
    shs:value "[Humidity > 70% => Open Device2]"^^xsd:string ;
    shs:Time "2019-11-09T16:46:15"^^xsd:dateTime .

shs:alertData1  a             prov:Entity ;
        shs:Time              "2019-11-09T16:46:00"^^xsd:dateTime ;
        shs:value             "{severity: warning, info: 'Humidity is 75%, higher than 60%. Please open the Device2!''}" ;
        prov:wasAttributedTo  shs:Server ;
        prov:wasDerivedFrom   shs:middleData3 ;
        prov:wasGeneratedBy   shs:analyze_alert1 .

shs:Server  a     prov:Agent , prov:SoftwareAgent ;
        shs:desc  "https://home.mi.com" .

shs:middleData2  a            prov:Entity ;
        shs:Time              "2019-11-09T16:45:30"^^xsd:dateTime ;
        shs:valueHum          "{nowHum: 75%, avgHum: 42%, maxHum: 75%, minHum: 40%}" ;
        shs:valueTep          "{nowTem: 25.1, avgTem: 24.6, maxTem: 25.1, minTem: 20.1}" ;
        prov:wasAttributedTo  shs:Server ;
        prov:wasGeneratedBy   shs:analyze_data2 .

shs:analyze_data2  a            prov:Activity ;
        # prov:used               shs:rawData_hum1 , shs:rawData_tep2 , shs:rawData_tep1 , shs:rawData_hum2 ;
        prov:wasAssociatedWith  shs:Server .

shs:analyze_alert1  a           prov:Activity ;
        prov:used               shs:middleData3 ;
        prov:wasAssociatedWith  shs:Server .

shs:create1  a                  prov:Activity ;
        prov:used               shs:configData2 , shs:middleData3 ;
        prov:wasAssociatedWith  shs:Server .

shs:middleData3  a            prov:Entity ;
        shs:Time              "2019-11-09T16:46:00"^^xsd:dateTime ;
        shs:valueHum          "{nowHum: 75%, avgHum: 42%, maxHum: 75%, minHum: 40%}" ;
        shs:valueTep          "{nowTem: 23.9, avgTem: 24.4, maxTem: 25.1, minTem: 20.1}" ;
        prov:wasAttributedTo  shs:Server ;
        # prov:wasDerivedFrom   shs:rawData_tep3 , shs:rawData_tep2 , shs:rawData_hum3 , shs:rawData_hum1 , shs:rawData_tep1 , shs:rawData_hum2 ;
        prov:wasGeneratedBy   shs:analyze_data3 .

shs:analyze_data3  a            prov:Activity ;
        # prov:used               shs:rawData_hum1 , shs:rawData_tep2 , shs:rawData_tep1 , shs:rawData_hum3 , shs:rawData_tep3 , shs:rawData_hum2 ;
        prov:wasAssociatedWith  shs:Server .

shs:middleData1  a            prov:Entity ;
        shs:Time              "2019-11-09T16:45:00"^^xsd:dateTime ;
        shs:valueHum          "{nowHum: 75%, avgHum: 42%, maxHum: 75%, minHum: 40%}" ;
        shs:valueTep          "{nowTem: 24.6, avgTem: 24.5, maxTem: 25.0, minTem: 20.1}" ;
        prov:wasAttributedTo  shs:Server ;
        prov:wasGeneratedBy   shs:analyze_data1 .

shs:analyze_data1  a            prov:Activity ;
        # prov:used               shs:rawData_hum1 , shs:rawData_tep1 ;
        prov:wasAssociatedWith  shs:Server .