@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix prov:  <http://www.w3.org/ns/prov#> .
@prefix foaf:  <http://xmlns.com/foaf/spec/20100809.html#> .
@prefix shs:   <http://example.org/#> .

shs:collect2  a                 prov:Activity ;
        prov:wasAssociatedWith  shs:Sensor2 .

shs:rawData_hum1  a           prov:Entity ;
        shs:collectAtTime     "2019-11-09T16:45:30"^^xsd:dateTime ;
        shs:value             "75%" ;
        prov:wasAttributedTo  shs:Sensor2 ;
        prov:wasGeneratedBy   shs:collect2 .

shs:Sensor1 a prov:Agent ;
    shs:sensorID "H1G1D1S001" .

shs:Sensor2 a prov:Agent ;
    shs:sensorID "H1G1D1S002" .

shs:Sensor1  prov:actedOnBehalfOf  shs:Device1 .

shs:rawData_tep2  a           prov:Entity ;
        shs:collectAtTime     "2019-11-09T16:45:30"^^xsd:dateTime ;
        shs:value             "25.1℃" ;
        prov:wasAttributedTo  shs:Sensor1 ;
        prov:wasGeneratedBy   shs:collect1 .

shs:rawData_hum2  a           prov:Entity ;
        shs:collectAtTime     "2019-11-09T16:46:30"^^xsd:dateTime ;
        shs:value             "42%" ;
        prov:wasAttributedTo  shs:Sensor2 ;
        prov:wasGeneratedBy   shs:collect2 .

shs:Sensor2  prov:actedOnBehalfOf  shs:Device1 .

shs:Device1  a        prov:Agent ;
        shs:address   "192.168.1.10" ;
        shs:deviceID  "H1G1D1" ;
        shs:protocol  "Zigbee" ;
        shs:state "Online"^^xsd:string .

shs:rawData_tep3  a           prov:Entity ;
        shs:collectAtTime     "2019-11-09T16:46:00"^^xsd:dateTime ;
        shs:value             "23.9℃" ;
        prov:wasAttributedTo  shs:Sensor1 ;
        prov:wasGeneratedBy   shs:collect1 .

shs:collect1  a                 prov:Activity ;
        prov:wasAssociatedWith  shs:Sensor1 .

shs:rawData_tep1  a           prov:Entity ;
        shs:collectAtTime     "2019-11-09T16:45:00"^^xsd:dateTime ;
        shs:value             "24.6℃" ;
        prov:wasAttributedTo  shs:Sensor1 ;
        prov:wasGeneratedBy   shs:collect1 .